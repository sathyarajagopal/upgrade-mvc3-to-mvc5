<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>UpgradeMvc3ToMvc5</id>
        <version>1.0.1</version>
        <title>Auto Upgrade MVC 3 To MVC 5</title>
        <authors>Sathya Prabu Rajagopal</authors>
        <owners>Sathya Prabu Rajagopal</owners>
        <projectUrl>https://github.com/sathyarajagopal/upgrade-mvc3-to-mvc5</projectUrl>
        <iconUrl>http://go.microsoft.com/fwlink/?LinkID=288859</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Package for auto upgrade of MVC 3.0.0.1 Application to MVC 5.2.3.0 Application</description>
        <summary>Package for auto upgrade of MVC 3.0.0.1 Application to MVC 5.2.3.0 Application</summary>
        <releaseNotes>Package for auto upgrade of MVC 3.0.0.1 Application to MVC 5.2.3.0 Application</releaseNotes>
        <language />
        <tags>MVC Upgrade</tags>
        <dependencies>
            <group targetFramework=".NETFramework4.5.2">
                <dependency id="Microsoft.AspNet.Mvc" version="[5.2.3]" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="tools\**" target="tools" />
        <file src="content\**" target="content" />
    </files>
